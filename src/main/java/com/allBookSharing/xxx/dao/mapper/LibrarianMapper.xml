<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.allBookSharing.xxx.dao.ILibrarianManagementDao">

<insert id="librarianinsert" parameterType="librarian">
 	INSERT INTO MEMBER(MB_ID,mb_pw,mb_area,ENABLED)
 	VALUES
 	(#{la_id},#{la_pw},#{la_loc},2)
 	</insert>
 	
 <insert id="librarianinsert2" parameterType="librarian">	
 	INSERT INTO authorities(authority,id)
 	VALUES
 	(2,#{la_id})
 </insert>
 <insert id="librarianinsert3" parameterType="librarian">	
 	INSERT INTO librarian(LA_ID,LA_LCODE)
 	VALUES
 	(#{la_id},#{la_lcode})
 
</insert>
	</mapper>